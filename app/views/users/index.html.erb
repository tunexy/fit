<h1>Listing users</h1>

<table>
  <tr>
    <th>Picture</th>
    <th>Name</th>
    <th>Sex</th>
    <th>Dob</th>
    <th>Address</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Expertise</th>
    <th>Password digest</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.picture %></td>
    <td><%= user.name %></td>
    <td><%= user.sex %></td>
    <td><%= user.dob %></td>
    <td><%= user.address %></td>
    <td><%= user.latitude %></td>
    <td><%= user.longitude %></td>
    <td><%= user.expertise %></td>
    <td><%= user.password_digest %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
